<form method="post" action="{{ path('adresse_delete', {'adresseId': adresse.adresseId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ adresse.adresseId) }}">
    <button class="btn btn-danger btn-block mt-2">Delete</button>
</form>
